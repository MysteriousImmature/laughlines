{
  "name": "LaughLines Jokes",
  "version": "3.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon32.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "clipboardWrite",
    "storage"
  ],
  "host_permissions": [
    "https://icanhazdadjoke.com/*",
    "https://official-joke-api.appspot.com/*"
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'; connect-src https://icanhazdadjoke.com https://official-joke-api.appspot.com"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "LaughLines@Auraaid.netlify.com",
      "strict_min_version": "70.0"
    }
  }
}
